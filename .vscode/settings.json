{
  "typescript.autoClosingTags": false,
  // Auto fix
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "css"
  ],
  "kiroAgent.configureMCP": "Disabled"
}
